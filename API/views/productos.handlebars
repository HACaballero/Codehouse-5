<div class="container">
	<a href="/" class="btn btn-primary">Nuevo producto</a>
	<table class="table">
		<thead>
			<tr>
				<th scope="col">#</th>
				<th scope="col">Nombre</th>
				<th scope="col">Precio</th>
				<th scope="col">Imagen</th>
			</tr>
		</thead>
		<tbody>
			{{#each list}}
				<tr>
					<td>{{id}}</td>
					<td>{{title}}</td>
					<td>{{price}}</td>
					<td><img src={{image}}  width="50" height="50"/></td>
				</tr>
			{{/each}}
		</tbody>
	</table>
</div>